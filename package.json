{
  "name": "next-of-dakshin-vaarahi-ai-os",
  "version": "1.0.0",
  "description": "AI-Powered Architectural Platform with Enhanced AI, Custom Training, and Future Advanced Capabilities",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "deploy:staging": "npm run build:production && echo 'Deploying to staging...'",
    "deploy:production": "npm run build:production && echo 'Deploying to production...'",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist node_modules/.vite",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
